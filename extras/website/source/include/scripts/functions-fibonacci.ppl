f(x)     = 0.0
f(x)[1:] = 1.0
f(x)[3:] = f(x-1) + f(x-2)

# Binet's Formula for the Fibonacci numbers
set numerics complex
binet(x) = Re((GoldenRatio**x - (1-GoldenRatio)**x) / sqrt(5))

set samples 100
set xrange [0:9.5]
set yrange [0:35]
set xlabel "$x$"
set ylabel "$y$"
set key bottom right
plot f(x) , binet(x)
